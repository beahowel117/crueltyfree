datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Product{
  id        Int @id @default(autoincrement())
  name      String
  desc      String
  price     Decimal
  quantity  Int
  image     String[]
  review    Json?
}

